            "apply_id": { "type": "number", "description": "申请用户id" },
            "create_at": { "type": "string", "description": "创建时间" },
            "update_at": { "type": "string", "description": "更新时间" }
          },
          "description": "朋友列表信息",
          "required": ["id", "agree_id", "apply_id", "create_at", "update_at"]
        },
